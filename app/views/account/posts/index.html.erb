<div class="col-md-12">
  <h2 class="text-center">我发表的文章</h2>
  <div>
    <%= link_to("返回主页", posts_path, class:"btn btn-primary btn-sm")%>
  </div>

  <div id="post-list">
    <% @posts.each do |post| %>
      <%= render :partial => "post", :locals => {:post => post }%>
    <% end %>
  </div>
</div>
